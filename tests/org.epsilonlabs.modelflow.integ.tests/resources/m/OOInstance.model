<?xml version="1.0" encoding="ASCII"?>
<OO:Package xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:OO="OO" name="org.animals">
  <contents xsi:type="OO:Datatype" name="String"/>
  <contents xsi:type="OO:Datatype" name="void"/>
  <contents xsi:type="OO:Datatype" name="int"/>
  <contents xsi:type="OO:Class" name="Pet" isAbstract="true">
    <features xsi:type="OO:Attribute" name="name" type="#//@contents.0" visibility="private"
        isMany="false"/>
    <features xsi:type="OO:Operation" name="sleep" type="#//@contents.1">
      <parameters name="duration" type="#//@contents.2"/>
    </features>
    <features xsi:type="OO:Operation" name="talk" type="#//@contents.1"/>
  </contents>
</OO:Package>
